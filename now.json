{
  "version": 2,
  "alias": [
    "snowfall"
  ],
  "builds": [
    {
      "src": "package.json",
      "use": "@now/static-build"
    }
  ],
  "github": {
    "silent": true
  },
  "routes": [
    { "src": "/(?:(.+\\.)((?!js$|css$|webmanifest$|html$|woff2$|ttf$|woff$|ico$|)[^.]*)|[^.]+)$", "dest": "index.html" }
  ]
}
